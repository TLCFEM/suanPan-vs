{
  "name": "suanpan",
  "displayName": "suanPan",
  "description": "Syntax highlighting and autocompletion for suanPan models",
  "icon": "images/suanPan-ua.png",
  "author": {
    "name": "Theodore Chang",
    "email": "tlcfem@gmail.com"
  },
  "pricing": "Free",
  "publisher": "tlc",
  "repository": {
    "type": "git",
    "url": "https://github.com/TLCFEM/suanPan-vs"
  },
  "version": "0.1.4",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:suanPan"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "suanPan",
      "properties": {
        "suanpan.path": {
          "type": "string",
          "default": "",
          "description": "Path to the suanPan executable."
        },
        "suanpan.directory": {
          "type": "string",
          "default": "",
          "description": "Working directory, leave empty to use the folder of the model file."
        },
        "suanpan.color": {
          "type": "boolean",
          "default": true,
          "description": "Colorized output."
        },
        "suanpan.verbose": {
          "type": "boolean",
          "default": false,
          "description": "Verbose output."
        },
        "suanpan.docker": {
          "type": "boolean",
          "default": false,
          "description": "Run the model in a docker container."
        },
        "suanpan.image": {
          "type": "string",
          "default": "tlcfem/suanpan",
          "description": "Docker image to use."
        }
      }
    },
    "commands": [
      {
        "command": "suanpan.run",
        "title": "suanPan: Run this model in suanPan"
      }
    ],
    "languages": [
      {
        "id": "suanPan",
        "aliases": [
          "suanPan"
        ],
        "extensions": [
          ".sp",
          ".supan"
        ]
      }
    ],
    "grammars": [
      {
        "language": "suanPan",
        "scopeName": "source.sp",
        "path": "./syntaxes/.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "suanPan",
        "path": "./syntaxes/snippets.json"
      },
      {
        "language": "suanPan",
        "path": "./syntaxes/nz_sections.json"
      },
      {
        "language": "suanPan",
        "path": "./syntaxes/eu_sections.json"
      },
      {
        "language": "suanPan",
        "path": "./syntaxes/us_sections.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.2.1",
    "eslint": "^9.17.0",
    "glob": "^11.0.0",
    "mocha": "^11.0.1",
    "typescript": "^5.7.2"
  },
  "volta": {
    "node": "22.12.0"
  }
}